{
  "GrayLogUrl": "http://graylog:7555",
  "MaxGrayLogConnectionAttempt": 100,
  "HostUrls": [ "http://0.0.0.0:5000/", "https://0.0.0.0:5001/" ],
  "AllowedOrigins": [ "http://*:9000/" ],
  "CorsOrigins": [ "http://127.0.0.1:9000", "http://localhost:9000", "http://192.168.1.203:9000" ],
  "AllowedHosts": "*",
  "ExchangeSettings": {
    "TestMode": false,
    "IndicatorDirectoryPath": "/exchange/service/indicators",
    "INIDirectoryPath": "/exchange/service/config",
    "MainCurrency": "EUR"
  },
  "Serilog": {
    "Using": [],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "/exchange/service/logs/log.txt",
          "outputTemplate": "{Timestamp} {Message}{NewLine:1}{Exception:1}"
        }
      },
      {
        "Name": "Graylog",
        "Args": {
          "hostnameOrAddress": "graylog",
          "port": "12201",
          "transportType": "Udp"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithProcessId",
      "WithThreadId"
    ],
    "Properties": {
      "ApplicationName": "Exchange.core"
    }
  }
}