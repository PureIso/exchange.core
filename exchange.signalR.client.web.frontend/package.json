{
    "name": "exchange",
    "productName": "Exchange",
    "version": "1.0.0",
    "description": "Cryptocurrency exchange API (Coinbase and Binance)",
    "main": "main.js",
    "scripts": {
        "start": "webpack serve --progress --config webpack/webpack-development.config.js",
        "start:docker": "webpack serve --progress --config webpack/webpack-docker.config.js",
        "test": "karma start",
        "test:watch": "npm run test --auto-watch --no-single-run",
        "e2e": "webdriver-manager update && concurrently --kill-others --success first \"webdriver-manager start\" \"protractor e2e/protractor.conf.js\"",
        "protractor": "protractor",
        "build:dist": "webpack --progress --mode production --config webpack/webpack-production.config.js",
        "electron:start": "npm run build:dist && electron \"/build/\"",
        "electron:build:x86": "npm run build:dist && build --projectDir \"/build/\" --ia32"
    },
    "author": "Olawale Egbeyemi",
    "repository": {
        "type": "git",
        "url": "https://github.com/PureIso/exchange.core.git"
    },
    "keywords": [
        "angular",
        "typescript",
        "rxjs",
        "sass",
        "redux",
        "karma",
        "webpack"
    ],
    "license": "MIT",
